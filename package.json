    "i18next": "^22.4.10",
    "next": "13.2.3",
    "next-i18next": "^13.1.6",
    "nprogress": "^0.2.0",
    "qs": "^6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
